<?xml version="1.0" encoding="UTF-8"?>
<phpunit
    colors="true"
    stopOnFailure="false"
	bootstrap="test/bootstrap.php"
>
   <filter>
        <whitelist>
            <directory suffix=".php">src</directory>
        </whitelist>
    </filter>
    <testsuites>
        <testsuite name="PHPQueue">
            <directory suffix="Test.php">test</directory>
        </testsuite>
    </testsuites>
</phpunit>